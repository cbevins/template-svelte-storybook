{
  "name": "starter-svelte",
  "version": "0.1.0",
  "description": "Starter repo for svelte-jest-storybook",
  "main": "index.js",
  "author": "Collin D. Bevins <cbevins@montana.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbevins/template-svelte-storybook.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "bugs": {
    "url": "https://github.com/cbevins/template-svelte-storybook/issues"
  },
  "homepage": "https://github.com/cbevins/template-svelte-storybook#readme",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --single",
    "storybook": "start-storybook -p 6005 -s public",
    "build-storybook": "build-storybook",
    "test": "jest --watchAll"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.stories\\.[jt]sx?$": "<rootDir>node_modules/@storybook/addon-storyshots/injectFileName",
      "^.+\\.svelte$": "jest-transform-svelte"
    },
    "moduleFileExtensions": [
      "js",
      "svelte",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|stylesheet)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/storybook-static/"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@rollup/plugin-commonjs": "16.0.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-essentials": "6.0.28",
    "@storybook/addon-knobs": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/addon-storyshots": "6.0.28",
    "@storybook/svelte": "6.0.28",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/svelte": "3.0.0",
    "babel-loader": "8.1.0",
    "eslint": "7.13.0",
    "eslint-config-standard": "16.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-standard": "4.0.2",
    "eslint-plugin-svelte3": "2.7.3",
    "jest": "26.6.3",
    "jest-transform-svelte": "2.1.1",
    "prettier-eslint": "11.0.0",
    "prettier-eslint-cli": "5.0.0",
    "react-is": "17.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.33.1",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "6.1.0",
    "rollup-plugin-terser": "7.0.2",
    "svelte": "3.29.4",
    "svelte-loader": "2.13.6"
  },
  "dependencies": {
    "sirv-cli": "1.0.8"
  }
}
